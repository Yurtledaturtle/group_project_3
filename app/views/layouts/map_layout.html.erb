  <script type="text/template" id='rating-template'>
      <td><%%= mood %></td>
      <td><%%= comment %></td>
      <td><%%= latitude %></td>
      <td><%%= longitude %></td>
  </script>

</head>
<body>

  <%= yield %>


</body>
</html>
